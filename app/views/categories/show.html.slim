p#notice = notice

p
  strong Name:
  = @category.name


table.table.table-striped.categories
  thead
    tr
      th Name
      th Rating

  tbody
    - @objects.each do |object|
      tr
        td = object.name
        - if object.image.ave_value
          td = object.image.ave_value.round(2)
        - else
          td 0





= link_to 'Back', categories_path
